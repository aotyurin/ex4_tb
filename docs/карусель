        float sellPrice8 = tickerDtoByPair8.getSellPrice();
        float quantity8 = (quantity7 - quantity7 * IExConst.STOCK_FEE) / sellPrice8;
        System.out.println(String.format("Покупаем по рынку: кол-во %1$s, цена %2$s", quantity8, sellPrice8));
        System.out.println("Текущие цены и объемы торгов: " + tickerDtoByPair8);
        String required_amount8 = exFactory.required_amount(pair8, quantity8);
        System.out.println(required_amount8);

        String pair9 ="ETH_RUB";

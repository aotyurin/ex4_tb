<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="285.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.ex4.apibt.view.LossOrderViewController">
    <children>
        <TableView fx:id="lossOrderTable" prefHeight="230.0" prefWidth="640.0">
            <columns>
                <TableColumn fx:id="pairLossOrderTableColumn" prefWidth="75.0" text="C1"/>
                <TableColumn fx:id="priceLossOrderTableColumn" prefWidth="75.0" text="Column X"/>
                <TableColumn fx:id="quantityLossOrderTableColumn" prefWidth="75.0" text="Column X"/>
                <TableColumn fx:id="typeLossOrderTableColumn" prefWidth="75.0" text="C2"/>
                <TableColumn fx:id="priceStepLossOrderTableColumn" prefWidth="75.0" text="Column X"/>
                <TableColumn fx:id="priceLossLossOrderTableColumn" prefWidth="75.0" text="Column X"/>
                <TableColumn fx:id="createdLossOrderTableColumn" prefWidth="75.0" text="Column X"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <contextMenu>
                <ContextMenu>
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#OnChangeLossOrderDialogShow" text="edit"/>
                        <MenuItem mnemonicParsing="false" onAction="#OnDeleteLossOrder" text="delete"/>
                    </items>
                </ContextMenu>
            </contextMenu>
        </TableView>
        <Pane prefHeight="42.0" prefWidth="640.0">
            <children>
                <Button layoutX="524.0" layoutY="16.0" mnemonicParsing="false" onAction="#btnOk" prefHeight="25.0" prefWidth="102.0" text="OK"/>
            </children>
        </Pane>
    </children>
</VBox>
